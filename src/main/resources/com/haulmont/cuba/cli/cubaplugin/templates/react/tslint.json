{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "interface-name": false,
    "member-access": false,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false,
    "max-classes-per-file": false,
    "member-ordering": false,
    "variable-name": false,
    "interface-over-type-literal": false
  }
}
